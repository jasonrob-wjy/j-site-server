#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jansonrob/code/mysite-express2/node_modules/.pnpm/opentype.js@0.7.3/node_modules/opentype.js/bin/node_modules:/Users/jansonrob/code/mysite-express2/node_modules/.pnpm/opentype.js@0.7.3/node_modules/opentype.js/node_modules:/Users/jansonrob/code/mysite-express2/node_modules/.pnpm/opentype.js@0.7.3/node_modules:/Users/jansonrob/code/mysite-express2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jansonrob/code/mysite-express2/node_modules/.pnpm/opentype.js@0.7.3/node_modules/opentype.js/bin/node_modules:/Users/jansonrob/code/mysite-express2/node_modules/.pnpm/opentype.js@0.7.3/node_modules/opentype.js/node_modules:/Users/jansonrob/code/mysite-express2/node_modules/.pnpm/opentype.js@0.7.3/node_modules:/Users/jansonrob/code/mysite-express2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../opentype.js@0.7.3/node_modules/opentype.js/bin/ot" "$@"
else
  exec node  "$basedir/../../../../../opentype.js@0.7.3/node_modules/opentype.js/bin/ot" "$@"
fi
